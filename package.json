{
  "name": "vibe-ui-core-test-build-template",
  "homepage": "http://shoeman22.github.io/vibe-ui-core/",
  "version": "20.0.0",
  "main": "./lib/index.js",
  "module": "./lib/index.es.js/index.js",
  "private": true,
  "engines": {
    "node": ">=10.11.0"
  },
  "author": {
    "name": "Andy Schuler",
    "email": "andy@leftshoedevelopment.com",
    "url": "http://shoeman22.github.io/vibe-ui-core/"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "vibes",
    "icentris"
  ],
  "dependencies": {
    "highlight.js": "^9.12.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --collectCoverageFrom=src/components/**/*.js --collectCoverageFrom=!src/components/**/index.js",
    "test:ci": "cross-env CI=true npm run test",
    "test:watch": "npm run test -- --watch",
    "prestart": "npm run gen:docs",
    "start": "npm-run-all --parallel test:watch gen:docs-watch start:docs",
    "start:docs": "node scripts/start.js",
    "gen:docs": "node scripts/generateComponentData.js",
    "gen:docs-watch": "npm run gen:docs -- --watch",
    "prebuild:docs": "npm run gen:docs",
    "build:docs": "node scripts/build.js",
    "predeploy:docs": "npm run build:docs",
    "deploy:docs": "gh-pages -d build",
    "prebuild:lib": "rimraf lib",
    "build:lib": "npm-run-all --parallel build:commonjs build:es build:css build:copy-files",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore spec.js",
    "build:es": "cross-env BABEL_MODULE_MODE=es NODE_ENV=production babel ./src/components --out-dir ./lib/index.es.js --ignore spec.js",
    "build:css": "cpx \"./src/components/**/*.css\" ./lib",
    "coverage": "npm run test -- --coverage",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "preversion": "npm run test:ci && npm run build:lib",
    "postversion": "git push && git push --tags",
    "lint": "eslint --ignore-path=.gitignore .",
    "lint:watch": "",
    "lint:fix": "npm run lint -- --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:ci",
      "pre-push": "npm run test:ci"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": "commonjs"
        }
      ],
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "chokidar": "^2.0.4",
    "coveralls": "^3.0.2",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "cross-spawn": "^6.0.5",
    "css-loader": "^1.0.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-watch": "^4.0.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^2.0.0",
    "fs-extra": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "2.29.0",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "npm-run-all": "^4.1.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "raf": "^3.4.0",
    "react-dev-utils": "^5.0.2",
    "react-docgen": "^2.21.0",
    "react-test-renderer": "^16.5.2",
    "resolve": "1.6.0",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "^1.1.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "1.3.2",
    "webpack-merge": "^4.1.4",
    "whatwg-fetch": "2.0.3"
  }
}
